this.div_Confirm_grd_SiInput_oncellclick = function(obj:nexacro.Grid,e:nexacro.GridClickEventInfo)
{
	var sCellProp = obj.getCellProperty("body", e.cell, "text");
	var sColNm = sCellProp.split(":")[1];
	
	if (!this.commUtil.getFormAuthority(262,6)) {
		if(this.ds_ProjectSiEmp.getColumn(this.ds_ProjectSiEmp.rowposition,"EDIT_FLAG")=="0"
			&& sColNm!="REMARKS")
		{
			alert(this.gfnGetTransTxt(1554,"투비소프트에서 배정한 건은 수정 할수 없습니다.\n컨설팅 관리자에게 문의 하세요"));
		}
	}
};


this.fn_setEditProjectSiEmpDate = function(nRow)
{
	var sEditFlag = this.ds_ProjectSiEmp.getColumn(nRow, "EDIT_FLAG");
	var sColor = this.ds_ProjectSiEmp.getColumn(nRow, "COLOR");
	var sRtnVal = "none";	
	var sCocd = nexacro.getApplication().gdsGlobal.getColumn(0, "CO_CD");
	
	// tobesoft는 현상황 동일 그외 CNC 투입인력 일자변경 권한만 수정 가능
	if (sCocd != "A01") {
		if (this.commUtil.getFormAuthority(262,6)) {		// CNC 투입인력 시작,종료일 변경 기능 추가
			
			if (sColor != "GREEN") {
				sRtnVal = "date";
			}
		} else {
			 if (sEditFlag == "1")	sRtnVal = "date";
		}
	} else {
		if (sEditFlag == "1")	sRtnVal = "date";
	}
	return sRtnVal;
