// SI투입중복여부 확인
this.fnCheckDuplicateSI = function(nRow, objDS, e) 
{
	var bPseFalg = objDS.getColumn(nRow, "PSE_FLAG");
	var nPseSeq = objDS.getColumn(nRow, "PSE_SEQ");
	var strEmoNo = objDS.getColumn(nRow, "EMP_NO");
	var strCoCd = objDS.getColumn(nRow, "CO_CD");
	var strStartDt = objDS.getColumn(nRow, "START_DATE");
	var strEndDt = objDS.getColumn(nRow, "END_DATE");
	//투입인력 중복확인시 변경하려는값으호 셋팅
	if(objDS==this.ds_ProjectSiEmp && e.columnid=="START_DATE")
		strStartDt = e.newvalue;
	else if(objDS==this.ds_ProjectSiEmp && e.columnid=="END_DATE")
		strEndDt = e.newvalue;
		
	var bNonResident = objDS.getColumn(nRow, "NONRESIDENT_FLAG");
	
	if (this.gfnIsEmpty(strEmoNo) 
		|| this.gfnIsEmpty(strStartDt) 
		|| this.gfnIsEmpty(strEndDt) 
		|| bNonResident=="1" 		//비상주는 체크안함
		|| bPseFalg =='1' 			//기존투입인력은 체크안함
		|| (strEmoNo==strCoCd))		//사번과 회사코드가 같으면 원청턴키이므로 중복체크 안함
	{
		return true;
	}
	
	this.gfnAddSendParameter("PSE_SEQ",		nPseSeq);	//투입인력에서 중복확인시 선택된 투입내역은 제외하고 조회
	this.gfnAddSendParameter("EMP_NO",		strEmoNo);
	this.gfnAddSendParameter("START_DATE",	strStartDt);
	this.gfnAddSendParameter("END_DATE",	strEndDt);
	this.gfnAddSendParameter("ROW",			nRow);
// 	this.gfnAddOutputDataset("ds_CheckDuplicateSI", "ds_output");
// 	this.gfnCommonTransaction("CheckDuplicateSI", "TR_RequestSIStaffReg_S02", null, false);
	this.gfnSetMap("techMapper", ['TR_RequestSIStaffReg_S02'], ['ds_CheckDuplicateSI']);
	this.gfnCommonTransaction("CheckDuplicateSI", "selectNextMultiList.do", null, false);	
	
	if (this.ds_CheckDuplicateSI.rowcount>0) {
		var nROW = this.ds_CheckDuplicateSI.getColumn(0, "ROW");
		var strEmpNm = objDS.getColumn(nROW, "EMP_NAME");
		//배정시 일정중복이면 배정인력 삭제해줌
		if(objDS==this.ds_ConfirmList)
		{
			objDS.setColumn(nROW, "EMP_NO");
			objDS.setColumn(nROW, "EMP_NAME");
			objDS.setColumn(nROW, "CO_NAME");
			objDS.setColumn(nROW, "CO_CD");
		}
		alert(this.gfnGetTransTxt(1533,"투입일정이 중복되었습니다.\n@1@ 투입일정을 확인하시기 바랍니다.",[strEmpNm]));
		return false;
	}
	else
		return true;
}

// 저장 (요청, 승인)
this.fnSave = function()
{	
	var strModelID = "";
	if (this.FV_FORM_TYPE == "TR") {
		if (!this.fn_getValidation()) return;
		
		//신규추가 저장시 담당자에게 문자를 보낸다
		if(this.ds_List.getRowType(0)==2)
		{
			if ( this.confirm(this.gfnGetTransTxt(1741,"SI인력투입 요청 SMS를 전송하시겠습니까?")))
			{
				var strMsg = this.gfnGetTransTxt(348, "SI인력요청") + "-" + this.ds_List.getColumn(0, "CLIENT_NAME") + "/" + this.ds_List.getColumn(0, "PROJECT_NAME") + "/"+this.gfnGetTransTxt(1099, "인원")+":" + this.ds_DetList.getRowCount();
				var sUserCode = "261,3"	//SI인력요청SMS 수신자
				if(this.sContractCoCd!="A01")
					sUserCode = "261,4"	//투비윈 SI인력요청SMS 수신자
				this.fnSendSMS(strMsg, "FORM_AUTH", sUserCode, "REQ_SI_STAFF");	
			}
		}
		
		this.gfnAddInputDataset("ds_List",		"dsInput");
		this.gfnAddInputDataset("ds_DetList",	"dsInput2");
		strModelID = "TR_RequestSupport_R02";
			
	} else if (this.FV_FORM_TYPE == "TC") {
		if (!this.fn_getValidation()) return;
		
		/*타사배정시 담당자에게 문자*/
		if(this.bOtherConfirmFlag)
		{
	 		if (this.confirm(this.gfnGetTransTxt(1742,"투비윈 배정 통보 SMS를 전송하시겠습니까?")))
	 		{
	 			var strMsg = this.gfnGetTransTxt(22824, "TOBE SI인력 배정") + "-" + this.ds_List.getColumn(0, "CLIENT_NAME") + "/" + this.ds_List.getColumn(0, "PROJECT_NAME");
	 			this.fnSendSMS(strMsg, "FORM_AUTH", "261,4", "ALLOC_SI_STAFF");	//투비윈 SI인력요청SMS 수신자
	 		}
			this.bOtherConfirmFlag=false;
		}
		/*SI투입일 중복 확인*/
		for(var i=0; i<this.ds_ConfirmList.getRowCountNF(); i++) 
		{
			if(this.fnCheckDuplicateSI(i, this.ds_ConfirmList))
				continue;
			else
				return;
		}
		
		var nCount = 0;
		/*SI투입 확정*/
		for(var i=0; i<this.ds_ConfirmList.getRowCountNF(); i++) 
		{
			if(!this.gfnIsEmpty(this.ds_ConfirmList.getColumnNF(i, "EMP_NO")) 
				&& this.ds_ConfirmList.getColumnNF(i, "PSE_FLAG")!="1")
			{
				this.fnSetDecideData(i);
				nCount = nCount+1;
			}
		}
		
		if (nCount > 0)
		{
			var strMsg = this.gfnGetTransTxt(22825, "SI투입확정") + "-" + this.ds_List.getColumn(0, "CLIENT_NAME") + "/" + this.ds_List.getColumn(0, "PROJECT_NAME")+"/"+nCount+this.gfnGetTransTxt(51, "건");
			//투비윈에서 SI투입확정시 SI인력요청SMS 수신자에게 SMS전송
			if(this.sContractCoCd == 'A01' && this.sLoginCoCd != 'A01')
			{
				this.fnSendSMS(strMsg, "FORM_AUTH", "261,3");	//SI인력요청SMS 수신자
			}
			//SI투입확정시 요청자에게 SMS전송
			this.fnSendSMS(strMsg, "EMP", this.ds_List.getColumn(0, "INSERT_EMP_NO"), "CONFIRM_SI_STAFF");		//요청자
		}
		
		this.gfnAddInputDataset("ds_ConfirmList", 	"dsInput");
		this.gfnAddInputDataset("ds_ProjectSiEmp", 	"dsInput1");
		this.gfnAddInputDataset("ds_DetList",		"dsInput2");
		this.gfnAddInputDataset("ds_List",			"dsInput3");
		
		strModelID = "TR_RequestSIStaffReg_R01";
	}
	
	this.gfnCommonTransaction("Save", strModelID);
}

this.fnSendSMS = function (strMsg, sUserKind, sUserCode, sAlimTalkService)
{
	var sSendType = "SELF";
	var sTelNoKind = "M";
	var arrSmsInfo = new Array();
		arrSmsInfo[0] = {};
		arrSmsInfo[0].pvUserKind = sUserKind;
		arrSmsInfo[0].pvUserCode = sUserCode;
		
	if (this.commUtil.getByteSize(strMsg) > 80) {
		strMsg = strMsg.substr(0, 80);
	}
	
	//this.gfnAddSmsList(arrSmsInfo);				// 수신자 처리
	this.gfnAddAlimTalkList(arrSmsInfo);			// 수신자 처리
	//this.gfnSendSms ("SendSMS", sSendType, sTelNoKind, "", strMsg, "");	// SMS 발송
	this.gfnSendAlimTalk ("SendSMS", sSendType, sTelNoKind, "", strMsg, "", sAlimTalkService);	// 알림톡 발송
}

// 콜백처리
this.fnCallBack = function(strSvcID, nErrorCode, strErrorMag)
{
	// 에러 예외처리
	if(nErrorCode != 0) {
		return;
	} else {
		if(strSvcID=="gfnGetCode") {
			this.fn_GetSkillCode();
			this.fnSearch();			
			this.ds_cmm_Check.addRow();
			
		} else if(strSvcID=="Select")	{
			this.sContractCoCd = this.ds_List.getColumn(this.ds_List.rowposition, "CO_CD");	//계약회사코드
			
			if (this.FV_FORM_TYPE == "TC" && this.commUtil.getFormAuthority(262,2) && !this.commUtil.gfnGetMobile())
			{
				if(this.bFormInit)
					this.btn_schdDown.click();
				this.bFormInit = false;
			}
			else if(this.sLoginCoCd!="A01")
			{
				this.fnSetFormatid();
			}
			//타사계약일경우 요청사항 수정 못함
			if(this.sContractCoCd!=this.sLoginCoCd && !this.gfnIsEmpty(this.sContractCoCd))
			{
				// 예외대상 목록
				var arrCompInfo = new Array();
				arrCompInfo[0] = this.div_Request.form.cmb_CheckType;	// 파일 버튼
				this.gfnCompDisable(this.div_Request.form, true, arrCompInfo);
				this.div_Req.form.grd_List.set_readonly(true);
			}
			/*투입이력이 없을경우 등록자외 SI 승인권한자는 삭제가능*/
			var strInsertEmp = this.ds_List.getColumn(this.ds_List.rowposition, "INSERT_EMP_NO");	//등록자
			if(this.ds_List.getColumn(0,"CNT") =="0" && (this.sLoginEmpNo==strInsertEmp || this.commUtil.getFormAuthority(262,2)))
				this.btn_Delete.set_enable(true);
				
			if(this.ds_List.getRowCount() <= 0)	{
				this.fnAddRow();
			}
		} else if(strSvcID=="ConfirmSelect")	{

		} else if (strSvcID=="getMM_DetList") {
			if (this.ds_MM.rowcount>0) {
				var nMM = this.ds_MM.getColumn(0, "MM");
				var nROW = this.ds_MM.getColumn(0, "ROW");
				this.ds_DetList.setColumn(nROW, "MEN_MONTH", nMM);
			}
		} else if (strSvcID=="getMM_ConfirmList") {
			if (this.ds_MM.rowcount>0) {
				var nMM = this.ds_MM.getColumn(0, "MM");
				var nROW = this.ds_MM.getColumn(0, "ROW");
				this.ds_ConfirmList.setColumn(nROW, "MEN_MONTH", nMM);
			}
		} else if (strSvcID=="getMM_ProjectSiEmp") {
			if (this.ds_MM.rowcount>0) {
				var nMM = this.ds_MM.getColumn(0, "MM");
				var nROW = this.ds_MM.getColumn(0, "ROW");
				this.ds_ProjectSiEmp.setColumn(nROW, "MEN_MONTH", nMM);
			}
		} else if (strSvcID=="CheckDuplicateSI") {

		} else if(strSvcID=="Save")	{
			if (this.FV_FORM_TYPE == "TR") {
				alert(this.gfnGetTransTxt(1534,"SI인력요청이 완료되었습니다."));
			} else {				
				alert(this.gfnGetTransTxt(1137,"저장되었습니다."));
			}
			this.fnSearch();
		} else if(strSvcID=="Delete")	{
			alert(this.gfnGetTransTxt(1223,"삭제되었습니다."));
			this.commUtil.popupClose("Delete");
		}
	}
}

