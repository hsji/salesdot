						 +"3. "+this.gfnGetTransTxt(22828, "연락처(전화/이메일)")+" : \n"
						 +"4. "+this.gfnGetTransTxt(22829, "수행장소")+" : \n"
						 +"5. "+this.gfnGetTransTxt(245, "특이사항")+" :\n";
	this.ds_List.setColumn(newrow, "WORK_CONTENTS",		strWorkContents);
	
	if(this.FV_FORM_TYPE == "TC") {
		this.ds_List.setColumn(newrow, "CONFIRM_EMP_NO", 	this.sLoginEmpNo);
		this.ds_List.setColumn(newrow, "CONFIRM_EMP_NAME",	this.sLoginEmpNm);
	}
	
	/*영업관리에서 인력요청시 계약내용 추가*/
	if(!this.gfnIsEmpty(this.parent.pvProjectCode))
	{
		this.ds_List.setColumn(newrow, "PROJECT_CODE",		this.parent.pvProjectCode);
		this.ds_List.setColumn(newrow, "PROJECT_NAME",		this.parent.pvProjectName);
		this.ds_List.setColumn(newrow, "CLIENT_CODE",		this.parent.pvClientCode);
		this.ds_List.setColumn(newrow, "CLIENT_NAME",		this.parent.pvClientName);
		this.ds_List.setColumn(newrow, "CONTRACT_NO",		this.parent.pvContractNo);
		this.ds_List.setColumn(newrow, "CONTRACT_NAME",		this.parent.pvContractName);
		this.ds_List.setColumn(newrow, "REQUEST_START_DATE",this.parent.pvStartDate);
		this.ds_List.setColumn(newrow, "REQUEST_END_DATE",	this.parent.pvEndDate);
		this.ds_List.setColumn(newrow, "AREA_CODE",			this.parent.pvAreacode);
		var ds = new nexacro.Dataset();
		ds.loadXML(this.parent.pvdsContractSi);

		for(var i=0; i<ds.getRowCount(); i++)
		{
			var nAddRow = this.ds_DetList.addRow();
			this.ds_DetList.setColumn(nAddRow, "ROLE_CODE", 	ds.getColumn(i, "ROLE_CODE"));
			this.ds_DetList.setColumn(nAddRow, "LEVEL_CODE", 	ds.getColumn(i, "LEVEL_CODE"));
			this.ds_DetList.setColumn(nAddRow, "UNIT_COST", 	ds.getColumn(i, "UNIT_COST"));
			this.ds_DetList.setColumn(nAddRow, "SKILL_1", 		this.parent.pvProductCode);
			this.ds_DetList.setColumn(nAddRow, "MEN_MONTH", 	ds.getColumn(i, "MEN_MONTH"));
			this.ds_DetList.setColumn(nAddRow, "START_DATE", 	ds.getColumn(i, "START_DATE"));
			this.ds_DetList.setColumn(nAddRow, "END_DATE", 		ds.getColumn(i, "END_DATE"));
		}
	}
	
	this.ds_List.set_enableevent(true);
	this.div_Request.form.div_Contract.setFocus();
}

// Validation check
 this.fn_getValidation = function()
 {
	var nRowCnt;
	var objDS;
	
	if (this.FV_FORM_TYPE == "TR") {
		nRowCnt = this.ds_DetList.rowcount;
		objDS = this.ds_DetList;
	}
	else if (this.FV_FORM_TYPE == "TC") {
		nRowCnt = this.ds_ConfirmList.getRowCountNF();
		objDS = this.ds_ConfirmList;
	}
		
	if ( this.gfnIsEmpty(this.ds_List.getColumn(0, "CONTRACT_NO"))) {
			alert(this.gfnGetTransTxt(1535,"계약을 선택해 주세요"));
			this.div_Request.form.div_Contract.setFocus();
			return false;
	}
		
	if ( this.gfnIsEmpty(this.ds_List.getColumn(0, "REQUEST_TYPE"))) {
			alert(this.gfnGetTransTxt(1536,"요청구분을 선택해 주세요"));
			this.div_Request.form.cmb_ReqType.setFocus();
			return false;
	}
	
	for (var i=0; i<nRowCnt; i++)
	{
		if ( this.gfnIsEmpty(objDS.getColumnNF(i, "ROLE_CODE"))) {
			alert(this.gfnGetTransTxt(1537,"역활을 입력하세요 "));
			objDS.set_rowposition(i);
			return false;
		}
		if ( this.gfnIsEmpty(objDS.getColumnNF(i, "LEVEL_CODE"))) {
			alert(this.gfnGetTransTxt(1538,"등급을 입력하세요 "));
			objDS.set_rowposition(i);
			return false;
		}
		
		var bCheckUnitCost = this.gfnIsEmpty(objDS.getColumnNF(i, "UNIT_COST"));
		if(this.sContractCoCd!="A01" && objDS == this.ds_ConfirmList)	//투비가 아닌경우 단가체크컬럼이 틀림
		{
			bCheckUnitCost = this.gfnIsEmpty(objDS.getColumnNF(i, "OTHER_UNIT_COST"));
		}
		
		//인력배정이 안되었으면 단가확인이 필요없음
		if ( bCheckUnitCost && !this.gfnIsEmpty(objDS.getColumnNF(i, "EMP_NO")) ) {
			alert(this.gfnGetTransTxt(1539,"단가를 입력하세요 "));
			objDS.set_rowposition(i);
			return false;
		}
		
		var strStartDt = objDS.getColumnNF(i, "START_DATE");
		var strEndDt = objDS.getColumnNF(i, "END_DATE");		
		if (strStartDt > strEndDt) {
			alert(this.gfnGetTransTxt(1540,"시작일이 종료일 보다 큽니다. 확인하세요 "));
			return false;
		}
		if ( this.gfnIsEmpty(strStartDt) && (!this.gfnIsEmpty(objDS.getColumnNF(i, "EMP_NO")) || this.FV_FORM_TYPE != "TC")) {
			alert(this.gfnGetTransTxt(1541,"시작일을 입력하세요 "));
			objDS.set_rowposition(i);
			return false;
		}
		if ( this.gfnIsEmpty(strEndDt) && (!this.gfnIsEmpty(objDS.getColumnNF(i, "EMP_NO")) || this.FV_FORM_TYPE != "TC")) {
			alert(this.gfnGetTransTxt(1542,"종료일을 입력하세요 "));
			objDS.set_rowposition(i);
			return false;
		}
		if ( this.gfnIsEmpty(this.ds_DetList.getColumn(i, "SKILL_1")) && this.FV_FORM_TYPE == "TR") {
			alert(this.gfnGetTransTxt(1543,"필수기술을 입력하세요 "));
			objDS.set_rowposition(i);
			return false;
		}
	}
	return true;
 }
  
