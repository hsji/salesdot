this.div_Confirm_grd_List_onexpandup = function(obj:nexacro.Grid,e:nexacro.GridMouseEventInfo)
{
	if (this.FV_FORM_TYPE == "TR")
		return;
	
	var sCellProp = obj.getCellProperty("body", e.cell, "text");	//제거 버튼
	if(!this.gfnIsEmpty(sCellProp))
	{	
		var sColNm = sCellProp.split(":")[1];
		var ds = obj.getBindDataset();
				
		if(sColNm == "EMP_NAME")
		{
			//인력배정
			if(this.gfnIsEmpty(this.ds_ConfirmList.getColumn(e.row, "EMP_NO")))
			{
				var sOtherFlag = this.ds_ConfirmList.getColumn(e.row, "OTHER_CONFIRM_FLAG");
				if(this.sLoginCoCd=="A01" && sOtherFlag=="1")
				{
					alert(this.gfnGetTransTxt(1550,"투비CNS 배정건은 인력배정을 할수 없습니다."));
					return;
				}
				var sArgument = {};
				this.commUtil.popupFindSiEmp(this, "popupFindSiEmp", "fnPopupCallBack", sArgument);
			}			
			else
			{
				if(ds.getColumn(e.row, "PSE_FLAG")=="1")
				{
					alert(this.gfnGetTransTxt(1551,"투입확정된 건은 배정인력을 변경할수 없습니다."));
					return;
				}
				/*배정인력 제거*/
				ds.setColumn(obj.getDatasetRow(e.row), "EMP_NO");
				ds.setColumn(obj.getDatasetRow(e.row), "EMP_NAME");
				ds.setColumn(obj.getDatasetRow(e.row), "CO_NAME");
				ds.setColumn(obj.getDatasetRow(e.row), "CO_CD");
			}			
		}
	}
}

