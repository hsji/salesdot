this.fnSetFormatid = function(bDown)
{
	var strFormatID = this.getCurrentLayoutID()=="portrait"?"mobile":"default";
	if(this.sLoginCoCd!="A01" && this.sContractCoCd!=this.sLoginCoCd)
		strFormatID = this.getCurrentLayoutID()=="portrait"?"mobile_ETC":"default_ETC";
	this.div_Req.form.grd_List.set_formatid(strFormatID);

	if (!this.gfnIsEmpty(bDown))
	{
		strFormatID = bDown?"TC":"default";
		if(this.sLoginCoCd!="A01")
			strFormatID = bDown?"TC_ETC":"default_ETC";
	}
 	this.div_Confirm.form.grd_List.set_formatid(strFormatID);
}

