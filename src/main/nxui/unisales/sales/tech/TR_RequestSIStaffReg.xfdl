			alert(this.gfnGetTransTxt(1549,"투입일이 종료일 보다 큽니다. 확인하세요 "));
			return false;
		}		
// 		if(strEndDt > strOrgEndDt)
// 		{	
// 			alert("투입일정 연장은 추가배정 통하여 추가투입을 하셔야 합니다.");
// 			return false;
// 		}
		if(strEndDt < strOrgEndDt)
		{
			return true;
		}
	} 
};

