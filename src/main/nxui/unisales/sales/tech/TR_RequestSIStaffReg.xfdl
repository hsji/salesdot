this.Grid_oncelldblclick = function(obj:nexacro.Grid,e:nexacro.GridClickEventInfo)
{
	var sCellProp = obj.getCellProperty("body", e.cell, "text");
	var sColNm = sCellProp.split(":")[1];	
	var objDB = obj.getBindDataset();	
		
	if(this.FV_FORM_TYPE="TC" && sColNm == "EMP_NAME")// obj.getBindCellIndex("body", "EMP_NAME") == e.cell)
	{
		var sEmpNo = objDB.getColumn(objDB.rowposition, "EMP_NO");
		var sEmpName = objDB.getColumn(objDB.rowposition, "EMP_NAME");
		if(!this.gfnIsEmpty(sEmpNo))
		{
			var Argument = {pvEMP_NO:sEmpNo, pvEMP_NAME:sEmpName};
			this.commUtil.popup (this, "TR_RequestOnsiteEduReg", "Common::CO_StaffInfo.xfdl", -1, -1, 986, 744, true, true, false, Argument, "fnPopupCallBack")
		}
	}
	else if(this.FV_FORM_TYPE="TC" && (sColNm=="UNIT_COST" || sColNm=="EXPENSE"))
	{
		var sPse_seq = objDB.getColumn(e.row, "PSE_SEQ");
		var sColor = objDB.getColumn(e.row, "COLOR");
		if(sColor=="GREEN")
		{
			sPse_seq = objDB.getColumn(e.row, "RELATED_PSE_SEQ");
			if(this.gfnIsEmpty(sPse_seq))
			{
				alert(this.gfnGetTransTxt(1553,"타사 계약건입니다."));
				return;
			}
		}
		/*프로젝트할당 팝업*/		
		var sProjectCd = objDB.getColumn(e.row, "PROJECT_CODE");
		this.commUtil.popup (this, "TR_RequestTechniqueReg", "Technique::TS_StaffSetProject.xfdl", -1, -1, 785, 558, true, true, false, {pv_pse_seq:sPse_seq, pv_project_code:sProjectCd}, "fnPopupCallBack");
	}
};

