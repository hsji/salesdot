// 그리드 column 변경
this.Dataset_oncolumnchanged = function(obj:nexacro.NormalDataset,e:nexacro.DSColChangeEventInfo)
{
	var strSvcID;
	if(obj==this.ds_DetList)
		strSvcID = "getMM_DetList";
	else if(obj==this.ds_ConfirmList)
		strSvcID = "getMM_ConfirmList";
	else if(obj==this.ds_ProjectSiEmp)
		strSvcID = "getMM_ProjectSiEmp";
		
	if(obj==this.ds_ConfirmList && e.columnid == "OTHER_CONFIRM_FLAG" && obj.getColumn(e.row, "OTHER_CONFIRM_FLAG")=="1")
		this.bOtherConfirmFlag = true;
	if(obj==this.ds_ConfirmList	&& (e.columnid == "UNIT_COST" ||e.columnid == "EXPENSE"))
	{
		var RSSD_SEQ = this.ds_ConfirmList.getColumn(this.ds_ConfirmList.rowposition, "RSSD_SEQ");
		var nFindRow = this.ds_DetList.findRow("RSSD_SEQ", RSSD_SEQ);
		var nDUnitCost = this.ds_DetList.getColumn(nFindRow, "UNIT_COST");
		
		var nCUnitCost = obj.getColumn(nFindRow, "UNIT_COST");
		var nCExpense = obj.getColumn(nFindRow, "EXPENSE");
		if(this.gfnIsEmpty(nCUnitCost))	nCUnitCost = 0;
		if(this.gfnIsEmpty(nCExpense))	nCExpense = 0;
		var nPerUnitCost = ((e.columnid=="UNIT_COST"?nCExpense:nCUnitCost)+e.newvalue)/nDUnitCost*100;
		
		this.ds_ConfirmList.setColumn(this.ds_ConfirmList.rowposition, "PER_UNIT_COST",nPerUnitCost);
	}
	
	// 시작일, 종료일 변경시 M/M계산 조회
	if (e.columnid == "START_DATE" || e.columnid == "END_DATE") {
		var strStartDt = obj.getColumn(e.row, "START_DATE");
		var strEndDt = obj.getColumn(e.row, "END_DATE");
		var strOrgEndDt = obj.getOrgColumn(e.row, "END_DATE");		
		
		this.fnGetMM(strSvcID, strStartDt, strEndDt, e.row);
	}
	
	// 투입이력 일정변경 처리 (변경사유 등록)
	if (obj==this.ds_ProjectSiEmp && (e.columnid == "START_DATE" ||e.columnid == "END_DATE") && !this.gfnIsEmpty(e.oldvalue))
	{
		var date = new Date();
		var sDate = (e.columnid == "START_DATE"?this.gfnGetTransTxt(38,"시작일"):this.gfnGetTransTxt(39,"종료일"));
		alert(this.gfnGetTransTxt(1637,"@1@을 변경하였습니다.\n저장하십시요!",[sDate]));
		var strReason = this.ds_ProjectSiEmp.getColumn(e.row, e.columnid == "START_DATE"?"START_CHANGE_REASON":"EARLY_OUT_REASON");
		var strText = "※ "+e.columnid == "START_DATE"?this.gfnGetTransTxt(38,"시작일"):this.gfnGetTransTxt(39,"종료일")+" "+this.gfnGetTransTxt(1953,"변경")+" ( "
					+this.gfnGetDateMaskFormatString(this.gfnStrToDate(e.oldvalue), "yyyy-MM-dd")+" → "
					+this.gfnGetDateMaskFormatString(this.gfnStrToDate(e.newvalue), "yyyy-MM-dd")+" ) "
					+" "+this.gfnGetTransTxt(336,"수정일")+" : "+this.gfnGetDateMaskFormatString(date, "yyyy-MM-dd")
					+" "+this.gfnGetTransTxt(337,"수정자")+" : "+this.sLoginEmpNm;
		
		if(this.gfnIsEmpty(strReason))
			strReason = strText;
		else
			strReason = strText+" \n\n"+strReason;
		
		this.ds_ProjectSiEmp.setColumn(e.row, e.columnid == "START_DATE"?"START_CHANGE_REASON":"EARLY_OUT_REASON", strReason);
// 		if(e.columnid == "END_DATE")
// 			this.ds_ProjectSiEmp.setColumn(e.row, "EARLY_OUT_FLAG", 	"1");
	}
}

this.ds_ProjectSiEmp_cancolumnchange = function(obj:nexacro.NormalDataset,e:nexacro.DSColChangeEventInfo)
{
	if(!this.fnCheckDuplicateSI(e.row, this.ds_ProjectSiEmp, e))	//투입인력의 일정조정시 중복일정 확인
		return false;
	else if (e.columnid == "START_DATE") {
		var strStartDt = e.newvalue;
		var strOrgStartDt = e.oldvalue;
		var strEndDt = obj.getColumn(e.row, "END_DATE");
		
		if (strStartDt > strEndDt) {
			alert(this.gfnGetTransTxt(1549,"투입일이 종료일 보다 큽니다. 확인하세요 "));
			return false;
		}
				
		if(strStartDt != strOrgStartDt)
		{
			return true;
		}
	} 
	else if (e.columnid == "END_DATE") {
		var strEndDt = e.newvalue;
		var strOrgEndDt = e.oldvalue;
		var strStartDt = obj.getColumn(e.row, "START_DATE");
		
