this.fnFormInit = function()
{	
	var app = nexacro.getApplication();	
	var sCocd = nexacro.getApplication().gdsUserInfo.getColumn(0, "CO_CD");
	this.stc_Name.set_text(this.gfnGetTransTxt(1920, "성명 : ") + app.gdsUserInfo.getColumn(0, "EMP_NM"));
	this.stc_Grade.set_text(this.gfnGetTransTxt(1919, "직급 : ") + app.gdsUserInfo.getColumn(0, "GRADE_NM"));
	this.stc_DeptNM.set_text(this.gfnGetTransTxt(1918, "부서명 : ") + app.gdsUserInfo.getColumn(0, "DEPT_NM"));
	
	if(app.gdsUserInfo.getColumn(0, "DUTY_CD")=="017"		//부문장
	||app.gdsUserInfo.getColumn(0, "DUTY_CD")=="009")		//본부장
	{
		if(sCocd == "J02") {
			this.DivSale.set_url("Base::BM_SalesTeam.xfdl");
		} else {
			this.DivSale.set_url("Base::BM_SalesLeader.xfdl");
		}
	}
	else if(app.gdsUserInfo.getColumn(0, "DUTY_CD")=="003")	//그룹장
	{
		if(sCocd == "J02") {
			this.DivSale.set_url("Base::BM_SalesTeam.xfdl");
		} else {
			this.DivSale.set_url("Base::BM_SalesLeader.xfdl");
		}
	}
	else if(app.gdsUserInfo.getColumn(0, "DUTY_CD")=="004")	//팀장
	{
		if(sCocd == "J02") {
			this.DivSale.set_url("Base::BM_SalesTeam.xfdl");
		} else {
			this.DivSale.set_top(4);
			this.DivSale.set_url("Base::BM_SalesLeaderTab.xfdl");
		}
	}
	else 													//팀원
	{
		this.DivSale.set_url("Base::BM_SalesTeam.xfdl");
	}	
	
	var date = new Date();	
	var nYear = date.getFullYear();
	var curDate = this.gfnGetDateMaskFormatString(date, "yyyyMMdd");  
	this.tab_SalesStatus.tpg_ApprovalDoc.form.rdo_SalesCheck.set_index(1);
	this.fnGetEmpList(nYear);
	this.fnRequestInfoDate(curDate);
};

this.setApprovalDoc_filter = function(strAL_STATUS)
{
	if(strAL_STATUS=="전체" || strAL_STATUS=="全体")
		this.dsApprovalDoc.filter("");
	else
		this.dsApprovalDoc.filter("AL_STATUS=='"+strAL_STATUS+"'");
};

