************************************************************************************************/
this.fnRequestInfo = function()
{			
	this.gfnAddSendParameter("KIND",	this.tab_SalesStatus.tpg_Manintenance.form.rdo_Enddate.value);
	this.gfnAddSendParameter("YYYYMM",	this.tab_SalesStatus.tpg_Manintenance.form.div_Search_yyyyMM.getYYYYMM());
	if(this.gfnIsEmpty(this.tab_SalesStatus.tpg_Manintenance.form.cb_EmpList.value))
	{
		var strDeptCD = this.dsEmpList.getColumn(this.tab_SalesStatus.tpg_Manintenance.form.cb_EmpList.index, "DEPT_CD");
		this.gfnAddSendParameter("DEPT_CD",	strDeptCD);
	}
	else
		this.gfnAddSendParameter("EMP_NO",	this.tab_SalesStatus.tpg_Manintenance.form.cb_EmpList.value);
		
	this.gfnAddOutputDataset("dsProcessStatus", "dsOutput1");
	this.gfnAddOutputDataset("dsSalesStatus", "dsOutput2"); 
	this.gfnAddOutputDataset("dsSalesCheck", "dsOutput3");
	this.gfnAddOutputDataset("dsManintenance","dsOutput4");
	this.gfnAddOutputDataset("dsManintenance2","dsOutput5");
	this.gfnAddOutputDataset("dsSaseList","dsOutput6");
	this.gfnAddOutputDataset("dsApprovalDoc","dsOutput7");

	var grdArray = new Array();
	grdArray[0] = this.grd_ProcessStatus;
	grdArray[1] = this.tab_SalesStatus.tpg_SalesStatus.form.grd_SalesStatus;
	grdArray[2] = this.tab_SalesStatus.tpg_SalesCheck.form.grd_SalesCheck;
	grdArray[3] = this.tab_SalesStatus.tpg_Manintenance.form.grd_Manintenance;
	grdArray[4] = this.tab_SalesStatus.tpg_ApprovalDoc.form.grd_ApprovalDoc;
	
	this.gfnCommonTransaction("RequestInfo", "BM_Sales_S01", grdArray);
};

this.fnRequestInfoDate = function(ISSUE_DATE)
{
	this.gfnAddSendParameter("ISSUE_DATE", ISSUE_DATE);
	
	this.gfnAddOutputDataset("dsRequestSi","dsOutput1");
	this.gfnAddOutputDataset("dsScheduleTeam","dsOutput2");
	this.gfnAddOutputDataset("dsSchedule","dsOutput3");
	
	var grdArray = new Array();
	grdArray[0] = this.tab_Schedule.tpg_Schedule.form.grd_Schedule;
	grdArray[1] = this.tab_Schedule.tpg_TeamSchedule.form.grd_TeamSchedule;
	grdArray[2] = this.tab_Schedule.tpg_RequestSi.form.grd_RequestSi;
	this.gfnCommonTransaction("RequestInfoDate", "BM_Sales_S02", grdArray);
	
};

this.fnMASearch = function()
{	
	this.gfnAddSendParameter("KIND",	this.tab_SalesStatus.tpg_Manintenance.form.rdo_Enddate.value);
	this.gfnAddSendParameter("YYYYMM",	this.tab_SalesStatus.tpg_Manintenance.form.div_Search_yyyyMM.getYYYYMM());
	if(this.gfnIsEmpty(this.tab_SalesStatus.tpg_Manintenance.form.cb_EmpList.value))
	{
		var strDeptCD = this.dsEmpList.getColumn(this.tab_SalesStatus.tpg_Manintenance.form.cb_EmpList.index, "DEPT_CD");
		this.gfnAddSendParameter("DEPT_CD",	strDeptCD);
	}
	else
		this.gfnAddSendParameter("EMP_NO",	this.tab_SalesStatus.tpg_Manintenance.form.cb_EmpList.value);
		
	this.gfnAddOutputDataset("dsManintenance"  , "dsOutput7");
	this.gfnAddOutputDataset("dsManintenance2" , "dsOutput8");
	
	this.gfnCommonTransaction("BM_SalesSerpport_S02", "BM_SalesSerpport_S02", this.tab_SalesStatus.tpg_Manintenance.form.grd_Manintenance);
};

this.fnGetEmpList = function(strYear)
{
	// 조건값 셋팅
	this.gfnAddSendParameter("YEAR",		strYear);
	// 조회용 Output dataset 세팅
	this.gfnAddOutputDataset("dsEmpList", 	"dsOutput");
	this.gfnCommonTransaction("GetEmpList", "SS_WeeklyReport_S02");
}

this.fnCallBack = function(strSvcID, nErrorCode, strErrorMag)
{
	// 에러 예외처리
	if(nErrorCode != 0) 
	{
		alert("strSvcID : " + strSvcID + "\n" + "nErrorCode : " + nErrorCode + "\n" + "strErrorMag : " + strErrorMag);
		return;
	}
	else 
	{
		if(strSvcID == "GetEmpList")
		{
			this.tab_SalesStatus.tpg_Manintenance.form.cb_EmpList.set_value(nexacro.getApplication().gdsUserInfo.getColumn(0, "EMP_NO"));
			if(this.tab_SalesStatus.tpg_Manintenance.form.cb_EmpList.index==-1)
				this.tab_SalesStatus.tpg_Manintenance.form.cb_EmpList.set_index(0);
			this.fnRequestInfo();
		}	
		else if(strSvcID == "RequestInfo")
		{
			this.setApprovalDoc_filter(this.tab_SalesStatus.tpg_ApprovalDoc.form.rdo_SalesCheck.text);
		}
		else if(strSvcID == "RequestInfoDate")
		{
		
		}
	}
};


